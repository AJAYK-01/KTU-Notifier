    "description": "App for SSC updates",
    "keywords": [
        "SSC",
        "telegram",
        "updates"
    ],
    "repository": "https://github.com/craziks-creator/notofssc",
    "env": {
        "TOKEN": {
            "description": "telegram both token",
            "required": "True"
        },
        "APIKEY": {
            "description": "Firebase api key",
            "required": "True"
        },
        "AUTHDOMAIN": {
            "description": "your Firebase auth domain",
            "required": "True"
        },
        "DATABASEURL": {
            "description": "Firebase DATABASE URL ",
            "required": "True"
        },
        "PROJECTID": {
            "description": " Your Firebase project id",
            "required": "True"
        },
        "STORAGEBUCKET": {
            "description": "Firebase storage bucket",
            "required": "True"
        },
    },
    "stack": "heroku-20",
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ],
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
